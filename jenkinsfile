pipeline {
    agent { label 'master'}

        stages {
            stage ('install') {
                steps {
                      sh "npm install"
                }
            }
            stage ('test') {
                steps {
                        sh "npm test"
                }
            }

            stage ('guardando artefacto') {
                steps {
                    sh 'ls'
                     sh 'aws s3 cp index.html s3://sa-practica1-junio21  --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers'
                }
            }


          
        }
}
