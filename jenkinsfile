pipeline {
    agent { label 'master'}

        stages {
            stage ('install') {
                steps {
                      sh "npm install"
                }
            }
            stage ('test') {
                steps {
                        sh "npm test"
                }
            }

            stage ('guardando artefacto') {
                steps {
                     sh 'aws s3 sync ./index.html s3://sa-practica1-junio21'
                }
            }


          
        }
}
